    gmail_service=app.gmail,\
    telegram_service=app.telegram,\
    weather_service=app.weather,\
    user_id=user.id\
)\
\
# Start scheduled cycle as background task\
asyncio.create_task(\
    intelligence.run_scheduled_cycle(interval_minutes=60)\
)\
\
# Handle Telegram callbacks\
@app.telegram.callback_handler\
async def handle_situation_action(callback_data):\
    action, situation_id = parse_callback(callback_data)\
    \
    if action == 'dismiss':\
        await intelligence.record_user_response(\
            situation_id=situation_id,\
            response='dismissed'\
        )\
    elif action == 'snooze':\
        await intelligence.record_user_response(\
            situation_id=situation_id,\
